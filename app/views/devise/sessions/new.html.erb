<h2>Sign in</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name), :'ng-controller' => 'Demo') do |f| %>
  <div><%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true, :'ng-model' =>'sign_in_user_name' %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password , :'ng-model' =>'sign_in_password' %></div>

  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
  <% end -%>

  <div><%= f.submit "Sign in" , :'ng-click' => 'sign_in_finction()'%></div>
<% end %>

<%= render "devise/shared/links" %>
